 <%= render 'books/book', book: @book %>
<div class="col-md-8 offset-md-1">
 <h2>Users</h2>
 <table class="table table-hover table-inverse">
   
    <tr>
        <th>image</th>
        <th>name</th>
        <th></th>
        <th colspan="3"></th>
    </tr>
   
    </thead>
  <tbody>
  <% @users.each do |user| %>
 
    <tr>
        <td><%= image_tag user.get_image(100,100) %></td>
            
        <td><%= link_to user_path(user.id) do %><%= user.name %><% end %></td>
        
        <td></td>
        
        <td><%= link_to user_path(user.id) do %><%= "Show" %><% end %></td>
        
    </tr>
    <% end %>
    </table>
    </tbody>
</div>